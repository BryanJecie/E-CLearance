{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "website"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": ["assets", "favicon.ico"],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/assets/plugins/pace/pace-theme-flash.css",
        "assets/assets/plugins/bootstrap/css/bootstrap.min.css",
        "assets/assets/plugins/font-awesome/css/font-awesome.css",
        "assets/assets/plugins/jquery-scrollbar/jquery.scrollbar.css",
        "assets/assets/plugins/select2/css/select2.min.css",
        "assets/assets/plugins/switchery/css/switchery.min.css",
        "assets/pages/css/pages-icons.css",
        "assets/custom/css/combobox.css",
        "assets/pages/css/themes/modern.css",
        "assets/custom/css/style.css"
      ],

      "scripts": [
        "assets/assets/plugins/pace/pace.min.js",
        "assets/assets/plugins/jquery/jquery-1.11.1.min.js",
        "assets/assets/plugins/modernizr.custom.js",
        "assets/assets/plugins/jquery-ui/jquery-ui.min.js",
        "assets/assets/plugins/tether/js/tether.min.js",
        "assets/assets/plugins/bootstrap/js/bootstrap.min.js",
        "assets/assets/plugins/jquery/jquery-easy.js",
        "assets/assets/plugins/jquery-unveil/jquery.unveil.min.js",
        "assets/assets/plugins/jquery-ios-list/jquery.ioslist.min.js",
        "assets/assets/plugins/jquery-actual/jquery.actual.min.js",
        "assets/assets/plugins/jquery-scrollbar/jquery.scrollbar.min.js",
        "assets/assets/plugins/select2/js/select2.full.min.js",
        "assets/assets/plugins/classie/classie.js",
        "assets/assets/plugins/switchery/js/switchery.min.js",
        "assets/pages/js/pages.min.js",
        "assets/assets/js/card.js",
        "assets/custom/js/combobox.js",
        "assets/custom/js/jQuery.print.js",
        "assets/pages/js/pages.min.js",
        "assets/assets/js/notifications.js",
        "assets/assets/js/form_elements.js",
        "assets/assets/js/scripts.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
